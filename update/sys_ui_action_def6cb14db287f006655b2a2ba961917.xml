        <script><![CDATA[function verifyRebuild() {
	var dialog = new GlideModal('confirm_yes_no', true, 'modal-sm');
	dialog.setTitle('Confirm Rebuild');
	dialog.setPreference('message', 'Are you sure you want to rebuild all metrics? This could take a while.');
	dialog.setPreference('onPromptYes', yesPrompt);
	dialog.render();
	
	function yesPrompt() {
		try {
			var ga = new GlideAjax('x_dehp_time_track.ReportingAjax');
			ga.addParam('sysparm_name', 'recalculateEpic');
			ga.getXML(complete);
			
		}
		catch(e) {
			alert(e);
		}
		function complete() {
			alert('Rebuilding Complete');
		}
	}
}]]></script>
