<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_fix">
    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description>Update any existing categoires that nave a "Null" value Charge Code to "Undetermined" charge code</description>
        <flush_cache>false</flush_cache>
        <name>Update Null Charge Code Categories</name>
        <record_for_rollback>false</record_for_rollback>
        <run_once>false</run_once>
        <script><![CDATA[var categories = new GlideRecord('x_dehp_time_track_time_categories');
categories.addEncodedQuery('code=NULL');
categories.query();

while (categories.next()) {
	categories.code = 'Undetermined';
	categories.update();
}]]></script>
