    <sys_script_fix action="INSERT_OR_UPDATE">
        <active>true</active>
        <before>false</before>
        <description/>
        <flush_cache>false</flush_cache>
        <name>Update Time Card State to Time Sheet Sta</name>
        <record_for_rollback>false</record_for_rollback>
        <run_once>true</run_once>
        <script><![CDATA[var timeSheets = new GlideRecord('x_dehp_time_track_time_sheet');
timeSheets.query(); 
while(timeSheets.next()){
	var timeCards = new GlideRecord('x_dehp_time_track_time_card');
	timeCards.addQuery('time_sheet', timeSheets.sys_id);
	timeCards.query();
	timeCards.setValue('state',  timeSheets.state);
	timeCards.updateMultiple();
}]]></script>
